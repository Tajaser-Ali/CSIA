         <image>
            <Image url="@../../../Images/dummy-removebg-preview.png" />
         </image></ImageView>

      <ImageView fx:id="usersImage" fitHeight="60" fitWidth="60" layoutX="18" layoutY="130" onMouseClicked="#takeToUsers" pickOnBounds="true" preserveRatio="true" />

      <Label layoutX="27.0" layoutY="85.0" text="Search" />
      <Label layoutX="31.0" layoutY="196.0" text="Users" />
      <Label layoutX="118.0" layoutY="14.0" prefHeight="71.0" prefWidth="170.0" text="Welcome Admin">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="logoutButton" layoutX="538.0" layoutY="3.0" mnemonicParsing="false" onAction="#onClickLogout" text="Logout" />
      <ImageView fx:id="editorImage" fitHeight="60" fitWidth="60" layoutX="16.0" layoutY="243.0" onMouseClicked="#takeToEditor" pickOnBounds="true" preserveRatio="true" />
