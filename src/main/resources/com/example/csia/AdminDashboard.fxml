<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csia.controllers.AdminDashboard_Controller">
   <children>
      <Line endX="-90.0" endY="-144.0" layoutX="177.0" layoutY="144.0" startX="-90.0" startY="256.0" />
